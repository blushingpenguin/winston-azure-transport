{
  "name": "winston-azure-transport",
  "description": "A winston transport for azure blobs",
  "version": "3.0.6",
  "private": false,
  "sideEffects": false,
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "main": "dist/module.js",
  "types": "dist/module.d.ts",
  "scripts": {
    "typecheck": "tsc --noEmit --noUnusedLocals",
    "build": "rimraf dist && tsc --build tsconfig.json && dependency-check --missing . "
  },
  "browserslist": {
    "development": [
      "last 2 chrome versions",
      "last 2 firefox versions",
      "last 2 edge versions"
    ],
    "production": [
      ">1%",
      "last 4 versions",
      "Firefox ESR",
      "not ie < 11"
    ]
  },
  "dependencies": {
    "@azure/identity": "^4.6.0",
    "@azure/storage-blob": "^12.26.0",
    "async": "npm:async-es@^3.2.6",
    "winston-transport": "^4.9.0"
  },
  "devDependencies": {
    "@types/async": "^3.2.24",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.10",
    "@types/qs": "^6.9.18",
    "@types/triple-beam": "^1.3.5",
    "dependency-check": "^4.1.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.3",
    "winston": "^3.17.0"
  },
  "peerDependencies": {
    "winston": "^3.17.0"
  },
  "license": "MIT",
  "author": "Mark Weaver <mark@blushingpenguin.com>",
  "keywords": [
    "winston",
    "transport",
    "winston3",
    "azure"
  ],
  "bugs": {
    "url": "https://github.com/blushingpenguin/winston-azure-transport/issues"
  },
  "homepage": "https://github.com/blushingpenguin/winston-azure-transport#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/blushingpenguin/winston-azure-transport.git"
  }
}
